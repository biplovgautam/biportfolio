{
  "version": 2,
  "builds": [
    {
      "src": "django_project/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticroot"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticroot/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "django_project/wsgi.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/vercel/path0",
    "DJANGO_SETTINGS_MODULE": "django_project.settings"
  }
}
